package Transactional_Management;

import org.springframework.stereotype.Component;


public class StudentBean {
int id;
String name;
public int getId() {
	return id;
}
public void setId(int id) {
	this.id = id;
}
public String getName() {
	return name;
}
public void setName(String name) {
	this.name = name;
}

}
